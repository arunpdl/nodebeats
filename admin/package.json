{
  "name": "admin2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:admin": "ng serve --app admin --proxy-config proxy.conf.json --host 0.0.0.0 --port 4500 --live-reload-port 49153",
    "start:client": "ng serve --app --aot client --proxy-config proxy.conf.json --host 0.0.0.0 --port 4200 --live-reload-port 49153",
    "build": "ng build --prod",
    "build:admin": "ng build --bh /admin/ --prod --app 1",
    "build:client": "ng build --prod --app 0 && ng build --prod --app 2 --output-hashing=false && npm run webpack:server",
    "build:all": "npm run build:client && npm run build:admin",
    "serve": "node dist/server.js",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/platform-server": "^5.2.10",
    "@angular/router": "^5.2.9",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "bootstrap": "4.1.1",
    "chart.js": "^2.7.2",
    "cloudinary": "^1.11.0",
    "cloudinary-jquery": "^2.1.6",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "moment": "^2.21.0",
    "ng2-charts": "^1.6.0",
    "popper.js": "^1.14.1",
    "rxjs": "^5.0.1",
    "sweetalert2": "^7.18.0",
    "ts-loader": "3.5.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.9",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.8.6",
    "@types/node": "^9.6.0",
    "codelyzer": "^4.2.1",
    "jasmine-core": "2.9.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.3.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "~2.6.2"
  }
}
